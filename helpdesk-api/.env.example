# Copy this file to .env and replace the values with your secrets
# Example for Neon DB (Postgres) - keep this file out of version control
DATABASE_URL="postgresql://username:password@host:5432/dbname?schema=public"
# JWT secret used for signing tokens. Use a long random string in production
JWT_SECRET="your_jwt_secret_here"
# Optional: number of bcrypt salt rounds
BCRYPT_SALT_ROUNDS=10
